{
  "name": "edgeworkers-jest-mocks",
  "version": "1.0.18",
  "description": "Akamai EdgeWorkers Jest mocks",
  "dependencies": {
    "@babel/core": "^7.17.8",
    "@babel/plugin-transform-runtime": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-typescript": "^7.16.7",
    "@babel/runtime": "^7.17.8",
    "babel-jest": "^27.5.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-xo": "^0.27.2",
    "eslint-plugin-prettier": "^3.4.1",
    "jest": "^27.5.1",
    "md5": "^2.3.0",
    "prettier": "^1.19.1"
  },
  "devDependencies": {
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleDirectories": [
      "node_modules",
      "./",
      "../../../src/edgeworkers/examples",
      "../../../edgeworkers"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error",
      "no-unused-vars": "warn"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "test": "jest"
  },
  "files": [
    "__mocks__",
    "babel.config.json",
    "package.*",
    "README.md"
  ],
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/akamai/edgeworkers-unittest.git"
  },
  "license": "Apache-2.0",
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ]
}
